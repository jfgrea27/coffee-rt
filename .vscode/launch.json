{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "cafe_order_api",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "cafe_order_api.app:app",
        "--host",
        "0.0.0.0",
        "--port",
        "8005",
        "--reload"
      ],
      "cwd": "${workspaceFolder}/backend/cafe_order_api/src",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/backend/cafe_order_api/src",
        "LOG_LEVEL": "DEBUG",
        "POSTGRES_HOST": "localhost",
        "POSTGRES_PORT": "5432",
        "POSTGRES_USER": "coffee-rt",
        "POSTGRES_PASSWORD": "coffee-rt_password",
        "POSTGRES_DB": "coffee-rt",
        "POSTGRES_SCHEMA": "coffee_rt",
        "REDIS_HOST": "localhost",
        "REDIS_PORT": "6379"
      },
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
